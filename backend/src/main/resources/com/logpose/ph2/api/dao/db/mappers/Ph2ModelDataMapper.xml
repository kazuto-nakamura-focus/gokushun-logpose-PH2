	<select id="selectModelAndDailyData" resultMap="ModelAndDailyDataEntity">
	    SELECT
			md.day_id as day_id,
			md.f_value as f_value,
			md.culmitive_cnopy_ps as culmitive_cnopy_ps,
			md.crown_leaf_area as crown_leaf_area,
			md.leaf_count as leaf_count,
			dd.date as date,
			dd.lapse_day as lapse_day,
			db.tm as tm,
			db.cdd as cdd,
			db.par as par,
			db.sun_time as sun_time,
			db.raw_cdd as raw_cdd
		FROM
			ph2_device_day dd
		JOIN
			ph2_model_data md ON dd.id = md.day_id
		JOIN
			ph2_daily_base_data db on dd.id = db.day_id
		WHERE
			dd.device_id = #{deviceId}
		AND
			dd.year = #{year}
		ORDER BY dd.lapse_day
	</select>
