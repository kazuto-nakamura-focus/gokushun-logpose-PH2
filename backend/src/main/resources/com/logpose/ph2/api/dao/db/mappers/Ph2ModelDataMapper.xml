	<select id="selectLeafModelDataByType" resultMap="LeafModelDataEntity">
		SELECT
			md.day_id as day_id,
			md.f_value as f_value,
			md.culmitive_cnopy_ps as culmitive_cnopy_ps,
			md.crown_leaf_area as crown_leaf_area,
			md.leaf_count as leaf_count,
			dd,date as date,
			dd.lapse_day as lapse_day,
			dd.has_real as has_real,
			rl.real_area as real_area,
			dd.source_type as source_type
		FROM
			ph2_device_day dd
		JOIN
			ph2_model_data md ON dd.id = md.day_id
		LEFT JOIN
			ph2_real_leaf_shoots_area rl ON dd.device_id = rl.device_id and dd.date=target_date
		WHERE
			dd.device_id = #{deviceId}
		AND
			dd.year = #{year}
		ORDER BY dd.lapse_day
	</select>
	
