	<select id="selectLapseDayByFValue" resultType="java.lang.Integer">
		select 
			ph2_device_day.lapse_day
		from
			ph2_device_day
		join
			ph2_model_data on ph2_device_day.id = ph2_model_data.day_id
		where
			ph2_device_day.device_id = #{deviceId}
			and
			ph2_device_day.year = #{year}
			and
			<choose>
				<when test="actualDate != null">
					ph2_device_day.date  = #{actualDate}
				</when>
				<otherwise>
					ph2_model_data.f_value <![CDATA[>=]]> #{fValue}
				</otherwise>
			</choose>
		order by ph2_device_day.lapse_day
	</select>
